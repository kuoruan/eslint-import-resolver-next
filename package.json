{
  "name": "eslint-import-resolver-next",
  "version": "0.6.0",
  "type": "module",
  "main": "./lib/index.cjs",
  "module": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "exports": {
    "types": "./lib/index.d.ts",
    "import": "./lib/index.js",
    "require": "./lib/index.cjs"
  },
  "sideEffects": false,
  "description": "The next resolver for `eslint-plugin-import` or `eslint-plugin-import-x`",
  "keywords": [
    "eslint",
    "eslint-plugin-import",
    "eslint-plugin-import-x",
    "eslint-import-resolver",
    "oxc-resolver"
  ],
  "repository": "https://github.com/kuoruan/eslint-import-resolver-next",
  "license": "MIT",
  "files": [
    "lib"
  ],
  "engines": {
    "node": ">=14.18.0"
  },
  "scripts": {
    "test": "vitest --run",
    "build": "rollup -c",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "prepublishOnly": "npm run test"
  },
  "dependencies": {
    "eslint-import-context": "^0.2.0",
    "is-bun-module": "^2.0.0",
    "js-yaml": "^4.1.0",
    "oxc-resolver": "^11.6.1",
    "stable-hash-x": "^0.2.0",
    "tinyglobby": "^0.2.14"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.28.0",
    "@eslint/js": "^9.33.0",
    "@rollup/plugin-typescript": "^12.1.4",
    "@tsconfig/node14": "^14.1.4",
    "@types/js-yaml": "^4.0.9",
    "@types/mock-fs": "^4.13.4",
    "@types/node": "^14.18.63",
    "eslint": "^9.33.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "globals": "^16.3.0",
    "mock-fs": "^5.5.0",
    "prettier": "^3.6.2",
    "rollup": "^4.46.3",
    "rollup-plugin-node-externals": "^8.0.1",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.40.0",
    "typescript-transform-paths": "^3.5.5",
    "vitest": "^3.2.4"
  }
